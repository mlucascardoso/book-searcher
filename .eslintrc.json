{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": "off",
        "indent": "off",
        "object-curly-spacing": "off",
        "one-var": "off",
        "require-jsdoc": "off"
    }
}